{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "max-classes-per-file": [
      true,
      10
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "eofline": false,
    "interface-over-type-literal": false
  }
}